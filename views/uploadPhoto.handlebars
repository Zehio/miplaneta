
<form role="form" enctype="multipart/form-data" method="POST"
action="/uploadPhoto/{year}/{month}">

    <label for="fieldName">Name</label>
    <input type="text" id="fieldName" name="name">

    <label for="fieldEmail">Email</label>
    <input type="email" required id="fieldName" name="email">


    <label for="fieldPhoto">Vacation photo</label>
    <input type="file" required accept="image/*"id="fieldPhoto" name="photo">

    <button type="submit">Submit</button>

</form>

